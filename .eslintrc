{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "semi": [2, "never"],
    "strict": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "func-names": 0,
    "global-require": 0,
    "react/jsx-filename-extension": 0,
    "no-plusplus": 0,
    "max-len": 0,
    "no-confusing-arrow": 0,
    "no-param-reassign": 0,
    "react/sort-comp": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "camelcase": 0
  }
}
