{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "semi": [2, "never"],
    "strict": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "func-names": 0,
    "global-require": 0,
    "react/jsx-filename-extension": 0
  }
}
